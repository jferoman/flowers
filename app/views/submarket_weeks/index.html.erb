<!-- START DATATABLE 1 -->
<h3>Temporadas por submercados</h3>

<div class="row">
 <div class="col-lg-6">
  <div class="panel panel-default">
   <div class="panel-heading">Importar temporadas por submercados</div>
   <div class="panel-body">
    <blockquote class="box-placeholder">
     <p>Importar archivo CSV
      <br>EL archivo debe contener las <% "hey" %> columnas:
      <br>date,submarket_name
    </p>
  </blockquote>
  <div class="row upload-box">
    <form id="select_button"  enctype="multipart/form-data" action="import_submarket_weeks" accept-charset="UTF-8" method="post">
      <div class="row">
       <div class="col-md-6">
        <input   accept="csv" class="form-control-file upload" id="file" name="file" type="file" />
      </div>
      <div class="col-md-2">
        <button type="submit" class="btn btn-primary start"><i class="fa fa-fw fa-upload"></i>
        </div>
      </div>
    </form>
  </div>
</div>
</div>
</div>

<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-default">
      <div class="panel-heading">
        Temporadas por submercados
        <small></small> |
        <%= link_to 'Nuevo Temporadas por submercados', new_submarket_week_path %>
      </div>
      <div class="panel-body">
        <table id="datatable1" class="table table-striped table-hover">
          <thead>
            <tr>
              <th class="sort-alpha">Semana</th>
              <% @submarket_names.each do |name| %>
                <th class="sort-alpha"> <%= name %></th>
              <% end %>
            </tr>
          </thead>
          <tbody>
            <tr class="gradeX">
              <td>d</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
<!-- END DATATABLE 1 -->
