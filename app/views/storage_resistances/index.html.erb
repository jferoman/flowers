
<div class="row">
   <div class="col-lg-4">
      <div class="panel panel-default">
         <div class="panel-heading">
           Tipos de resistencia al guarde |
           <small></small> 
           <%= link_to 'Nuevo tipo de resistencia al guarde', new_storage_resistance_type_path %>
         </div>
         <div class="panel-body">
            <table id="datatable1" class="table table-striped table-hover">
               <thead>
                  <tr>
                     <th class="sort-alpha">Nombre</th>
                     <th class="sort-alpha">Acciones</th>
                  </tr>
               </thead>
               <tbody>
                <% @storage_resistance_types.each do |storage_resistance_type| %>
                  <tr class="gradeXa">
                     <td> <%= storage_resistance_type.name %> </td>
                     <td>
                       <%= link_to 'Editar', edit_storage_resistance_type_path(storage_resistance_type) %> |
                       <%= link_to 'Eliminar', storage_resistance_type ,  method: :delete, data: { confirm: 'Desea eliminar el tipo de resistencia al guarde?' } %>
                     </td>
                  </tr>
               <% end %>
               </tbody>
            </table>
         </div>
      </div>
   </div>
</div>
<div class="row">
   <div class="col-lg-12">
      <div class="panel panel-default">
         <div class="panel-heading">
           Resistencias al guarde |
           <small></small> 
           <%= link_to 'Nueva resistencia al guarde', new_storage_resistance_path %>
         </div>
         <div class="panel-body">
            <table id="datatable2" class="table table-striped table-hover">
               <thead>
                  <tr>
                     <th class="sort-alpha">Tipo de resistencia al guarde</th>
                     <th class="sort-alpha">Semana</th>
                     <th class="sort-alpha">Porcentaje perdido</th>
                     <th class="sort-alpha">Acciones</th>
                  </tr>
               </thead>
               <tbody>
                <% @storage_resistances.each do |storage_resistance| %>
                  <tr class="gradeX">
                     <td> <%= storage_resistance.name %> </td>
                     <td> <%= storage_resistance.week_number %></td>
                     <td> <%= storage_resistance.lost_percentage %></td>
                     <td>
                       <%= link_to 'Editar', edit_storage_resistance_path(storage_resistance) %> |
                       <%= link_to 'Eliminar', storage_resistance,  method: :delete, data: { confirm: 'Desea eliminar el resistencia al guarde?' } %>
                     </td>
                  </tr>
               <% end %>
               </tbody>
            </table>
         </div>
      </div>
   </div>
</div>
<h3>Importar Tipos De Resistencias al Guarde</h3>
<blockquote class="box-placeholder">
   <p>Importar archivo CSV
      <br>EL archivo debe contener las <% "hey" %> columnas:
      <br>nombre
   </p>
</blockquote>
<div class="row upload-box">
   <form id="select_button"  enctype="multipart/form-data" action="storage_resistances/csv_import" accept-charset="UTF-8" method="post">
      <div class="row">
         <div class="col-md-3">
            <input   accept="csv" class="form-control-file upload" id="file" name="file" type="file" />
         </div>
         <div class="col-md-2">
            <button type="submit" class="btn btn-primary start"><i class="fa fa-fw fa-upload"></i>
         </div>
      </div>
   </form>
</div>