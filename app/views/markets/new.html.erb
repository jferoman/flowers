<%= form_for @market do |f| %>
  <div class="form-group">
    <%= f.label :name, 'Mercado: ' %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :code, 'Codigo: ' %>
    <%= f.text_field :code, class: 'form-control' %>
  </div>
  <div>
    <%= f.submit 'Crear Mercado' %>
  </div>
<% end %>
